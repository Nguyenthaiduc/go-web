package main

type myStruct struct {
	FirstName string
}
//print firstname
func  (m *myStruct) printFirstName() string {
	return m.FirstName
}



func Struct_Function(){
	var myVar myStruct
	myVar.FirstName = "Hello"

	myVar2:= myStruct{
		FirstName : "Hi"
	}

	log.Println("my var is set to",myVar.printFirstName())
	log.Println("my var2 is set to",myVar2.printFirstName())

}
